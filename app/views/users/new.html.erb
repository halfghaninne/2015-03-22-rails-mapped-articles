<%= render :partial => '/shared/mobile_nav' %>

<%= render :partial => '/shared/main_nav' %>

<div class="user_signup">
  <h2>Sign up</h2>
  <br>

  <%= form_for @user do |f| %>
  	<%= f.label "username", "Username:" %>
      <%= f.text_field("username") %><br><br>
  	<%= f.label "password", "Password:" %>
      <%= f.password_field("password") %><br><br>
      <%= f.submit "Register" %>
  <% end %>
</div>