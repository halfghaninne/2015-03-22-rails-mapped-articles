Sign up

<%= form_for @user do |f| %>
	<%= f.label "username", "Username:" %>
    <%= f.text_field("username") %><br>
	<%= f.label "email", "Email:" %>
	<%= f.text_field("email") %><br>
	<%= f.label "password", "Password:" %>
    <%= f.password_field("password") %><br>
    <%= f.submit "Register" %>
<% end %>