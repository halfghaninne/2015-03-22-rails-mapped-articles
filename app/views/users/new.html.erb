<%= render :partial => '/shared/main_nav' %>

<div class="user_signup">
  Sign up

  <%= form_for @user do |f| %>
  	<%= f.label "username", "Username:" %>
      <%= f.text_field("username") %><br>
  	<%= f.label "password", "Password:" %>
      <%= f.password_field("password") %><br>
      <%= f.submit "Register" %>
  <% end %>
</div>